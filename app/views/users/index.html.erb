<p id="notice"><%= notice %></p>

<h1>High Scores</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each_with_index do |user, index| %>
      <tr>
        <td><%= index + 1 %>.</td>
        <td><%= user.username %></td>
        <td class="score"><%= user.high_score %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

